TWILIO_SERVICE_RETRY_LIMIT=5
TWILIO_SERVICE_MIN_BACKOFF=100
TWILIO_SERVICE_MAX_BACKOFF=300
TWILIO_SYNC_MAX_BACKOFF=2000
TWILIO_SYNC_MIN_BACKOFF=1000


# COMMON - USED IN VIDEO APIS AND WEB WIDGET
TWILIO_API_KEY=<YOUR_TWILIO_API_KEY>
TWILIO_API_SECRET=<YOUR_TWILIO_API_SECRET>

# FUTURE CC WIDGET
EMAIL_FROM=<YOUR_EMAIL_FROM>
ADDRESS_SID=<YOUR_TWILIO_FLEX_WEBCHAT_ADDRESS_SID>

# AI RESPONSES
OPENAI_API_KEY=<YOUR_OPENAI_API_KEY>
