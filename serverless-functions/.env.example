# ACCOUNT SPECIFIC SETTINGS ONLY REQUIRED WHEN RUNNING LOCALLY
ACCOUNT_SID=<YOUR_ACCOUNT_SID>
AUTH_TOKEN=<YOUR_AUTH_TOKEN>

# WHEN RUNNING SERVERLESS LOCALLY, THE PAYLOAD LOGS SEEN IN CONSOLE ARE NOT AVAILABLE
# SO EXTRA LOGGING IS USEFUL FOR DEBUGGING
ENABLE_LOCAL_LOGGING=true

# GENERAL 
TWILIO_FLEX_WORKSPACE_SID=<YOUR_TWILIO_FLEX_WORKSPACE_SID>
TWILIO_FLEX_SYNC_SID=<YOUR_TWILIO_FLEX_SYNC_SID>
TWILIO_FLEX_CHAT_SERVICE_SID=<YOUR_TWILIO_FLEX_CHAT_SERVICE_SID>
